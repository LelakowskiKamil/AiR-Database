<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="300.0" maxWidth="300.0" minHeight="300.0" minWidth="300.0"
           prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
           fx:controller="sample.addStudentControler">
    <children>
        <TextField fx:id="fieldStudentCard" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" promptText="StudentCard"
                   StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" top="140.0"/>
            </StackPane.margin>
        </TextField>
        <TextField fx:id="fieldMajor" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" promptText="Major"
                   StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" top="110.0"/>
            </StackPane.margin>
        </TextField>
        <TextField fx:id="fieldEmail" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" promptText="Email"
                   StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" top="80.0"/>
            </StackPane.margin>
        </TextField>
        <TextField fx:id="fieldSurname" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" promptText="Surname"
                   StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" top="50.0"/>
            </StackPane.margin>
        </TextField>
        <TextField fx:id="fieldName" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" promptText="Name"
                   StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" top="20.0"/>
            </StackPane.margin>
        </TextField>
        <RadioButton fx:id="radioMale" mnemonicParsing="false" onAction="#setSex" text="male"
                     StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" right="20.0" top="170.0"/>
            </StackPane.margin>
        </RadioButton>
        <RadioButton fx:id="radioFemale" accessibleText="female" mnemonicParsing="false" onAction="#setSex"
                     text="female" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="100.0" top="170.0"/>
            </StackPane.margin>
        </RadioButton>
        <Button fx:id="buttonAddStudent" mnemonicParsing="false" onAction="#addStudent" text="ADD"
                StackPane.alignment="TOP_CENTER">
            <StackPane.margin>
                <Insets top="200.0"/>
            </StackPane.margin>
        </Button>
        <Label fx:id="labelText" text="Label" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="20.0" top="200.0"/>
            </StackPane.margin>
        </Label>
    </children>
</StackPane>
